        "teardown-browser": "node ./build/scraping/teardownBrowser.js"
    },
    "keywords": [
        "bot",
        "telegram",
        "node",
        "job",
        "work",
        "employment",
        "recruitment"
    ],
    "author": "Jan Librowski",
    "license": "MIT",
    "dependencies": {
        "dotenv": "^8.2.0",
        "lodash": "^4.17.20",
        "puppeteer": "^5.5.0",
        "telegraf": "^4.0.1"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.167",
        "@types/node": "^14.14.12",
        "@types/puppeteer": "^5.4.2",
        "@typescript-eslint/eslint-plugin": "^4.9.1",
        "@typescript-eslint/parser": "^4.9.1",
        "eslint": "^7.15.0",
        "husky": "^5.0.6",
        "mongodb": "^3.6.3",
        "nodemon": "^2.0.6",
        "typescript": "^4.1.3"
    }
