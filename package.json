{
    "name": "rabot",
    "version": "1.0.0",
    "description": "Your personal job-seeking bot",
    "main": "index.ts",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "yarn build --watch & yarn start",
        "start": "nodemon -r dotenv/config ./build/index.js",
